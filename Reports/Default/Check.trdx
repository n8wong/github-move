      </GroupHeader>
      <GroupFooter>
        <GroupFooterSection Height="1in" Name="groupFooterSection1">
          <Style Visible="False" />
        </GroupFooterSection>
      </GroupFooter>
      <Groupings>
        <Grouping Expression="= Fields.CheckNumber" />
      </Groupings>
    </Group>
  </Groups>
  <ReportParameters>
    <ReportParameter Name="AuditId" Type="Integer" Text="Audit" Visible="True">
      <Value>
        <String>11</String>
      </Value>
    </ReportParameter>
    <ReportParameter Name="CheckNumber" Type="Integer" Text="Check" Visible="True" />
  </ReportParameters>
</Report>