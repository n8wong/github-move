<?xml version="1.0" encoding="utf-8"?>
<Report DataSourceName="CheckPayments" Width="6.5in" Name="Check" xmlns="http://schemas.telerik.com/reporting/2020/2.0">
  <DataSources>
    <SqlDataSource QueryDesignerState="null" ParameterValues="{&quot;@AuditId&quot;:&quot;1&quot;,&quot;@CheckNumber&quot;:&quot;1&quot;}" ConnectionString="JmsDb" SelectCommand="SELECT a.Id AS AuditId, r.CheckNumber&#xD;&#xA;, REPLACE(c.FirstName + ' ' + c.MiddleName + ' ' + c.LastName, ' ', ' ') AS FullName&#xD;&#xA;, r.Amount, a.CreatedDateTimeUtc&#xD;&#xA;FROM PaymentAudits a&#xD;&#xA;INNER JOIN PaymentRegisterEntries AS r ON a.Id = r.PaymentAuditId&#xD;&#xA;INNER JOIN Candidates AS c ON r.CandidateId = c.Id&#xD;&#xA;WHERE a.Id = @AuditId AND r.CheckNumber = @CheckNumber AND r.Amount - r.DonationAmount &gt; 0" Name="CheckPayments">
      <Parameters>
        <SqlDataSourceParameter DbType="Int16" Name="@AuditId">
          <Value>
            <String>= Parameters.AuditId.Value</String>
          </Value>
        </SqlDataSourceParameter>
        <SqlDataSourceParameter DbType="Int16" Name="@CheckNumber">
          <Value>
            <String>= Parameters.CheckNumber.Value</String>
          </Value>
        </SqlDataSourceParameter>
      </Parameters>
    </SqlDataSource>
  </DataSources>
  <Items>
    <PageHeaderSection Height="1in" Name="pageHeaderSection1">
      <Style Visible="False" />
    </PageHeaderSection>
    <DetailSection Height="2in" Name="detailSection1">
