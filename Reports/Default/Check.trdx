      <GroupFooter>
        <GroupFooterSection Height="1in" Name="groupFooterSection">
          <Style Visible="False" />
        </GroupFooterSection>
      </GroupFooter>
    </Group>
    <Group Name="group1">
      <GroupHeader>
        <GroupHeaderSection Height="1in" Name="groupHeaderSection1">
